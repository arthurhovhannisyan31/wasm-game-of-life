{
  "name": "wasm-game-of-life",
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": "^22.9.0"
  },
  "scripts": {
    "build": "yarn build:rs && yarn build:fe",
    "build:rs": "configs/scripts/build-wasm.sh",
    "build:fe": "configs/scripts/build-web.sh",
    "dev": "webpack serve --config configs/webpack/webpack.dev.js",
    "watch:rs": "nodemon --polling-interval 1000 -e rs --exec \"yarn build:rs --debug\"",
    "serve": "nodemon -r dotenv/config --config configs/nodemon-prod.json server.js",
    "prepare": "corepack install && husky",
    "lint": "yarn eslint '**/*.{js,ts,tsx}' --cache --quiet",
    "lint:warn": "yarn lint --max-warnings 100",
    "lint:staged": "./configs/scripts/lint-staged.sh",
    "types": "NODE_OPTIONS=--max-old-space-size=8196 tsc -p tsconfig.json --noEmit",
    "cargo-docs": "cargo doc --open --no-deps",
    "test:rs": "wasm-pack test --chrome --headless"
  },
  "dependencies": {
    "@arthurhovhannisyan31/simple-service-worker": "^1.0.6",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/styled-engine": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/types": "^7.4.4",
    "lodash-es": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.1",
    "eslint-config-next": "^15.3.4",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-deprecate": "^0.8.7",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "globals": "^16.3.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.10",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "webpack": "^5.99.9",
    "webpack-assets-manifest": "^6.2.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
